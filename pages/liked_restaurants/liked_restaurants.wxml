<block wx:for="{{restaurants}}">
  <view class="weui-form-preview__hd">
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">{{item.name}}</view>
      <view class="weui-form-preview__value_in-hd">
        <button bindtap='unlike' data-id="{{item.id}}">unlike</button>
      </view>
    </view>
  </view>
</block>